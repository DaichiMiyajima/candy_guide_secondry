<!DOCTYPE html>
<html lang="en" ng-app="mapper">
<head>
    <meta charset="utf-8">
    <meta name="viewport"    content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author"      content="Daichi Miyajima">
    <meta property="og:title" content="" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="" />
    <meta property="og:image" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:description" content="" />
    <title>Guide</title>
    <link rel="canonical" href="http://guide.com" />
    <link rel="shortcut icon" href="">
    <!-- firebase url -->
    <script src="https://cdn.firebase.com/js/client/2.4.2/firebase.js"></script>
    <!-- GeoFire -->
    <script src="https://cdn.firebase.com/libs/geofire/4.1.1/geofire.min.js"></script>
    <!-- CSS  -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" media="screen" href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,700">
    <link rel="stylesheet" href="../public/css/index.css">
    <!-- sweet alert-->
    <script src="../public/ext/sweetalert/sweetalert.min.js"></script>
    <link rel="stylesheet" type="text/css" href="../public/ext/sweetalert/sweetalert.css">
    <!-- jquery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.js"></script>
    <!-- Angular.js -->
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.0/angular.min.js"></script>
    <script src="https://cdn.firebase.com/libs/angularfire/1.2.0/angularfire.min.js"></script>
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css">
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>
    
</head>
<body class="">
    <div ng-controller="candyController">
        <div class="mdl-tabs mdl-js-tabs mdl-js-ripple-effect">
            <!-- Modal Structure -->
            <div id="modal1" class="modal bottom-sheet">
                <div class="modal-content">
                    <!-- message tab -->
                    <div style="padding-top: 1px;">
                        <div ng-repeat="message in messages | orderBy:'-time'">
                            <div ng-if="message.kind === 'message'">
                                  <div id="toast-container-chat" class="toast-top-right" aria-live="polite" role="alert" style="position: initial !important;margin: 5px 5px 5px 5px;">
                                    <div class="toast toast-info" style="display: block; text-align: left;float: left;width: 100%;">
                                        <div class="toast-message">[{{message.name}}] : {{message.message}}</div>
                                    </div>
                                    <span>{{ message.time | date:'MM/dd/yyyy hh:mm' : timezone}}</span>
                                </div>
                            </div>
                            <div ng-if="message.kind === 'attend' || message.kind == 'meetup'">
                                <div id="toast-container-chat" class="toast-top-right" aria-live="polite" role="alert" style="position: initial !important;margin: 5px 5px 5px 5px;">
                                    <div class="toast toast-success" style="display: block;width: 100%;">
                                        <div class="toast-message">{{message.message}}</div>
                                    </div>
                                </div>
                            </div>
                            <div ng-if="message.kind === 'meetupremove'">
                                <div id="toast-container-chat" class="toast-top-right" aria-live="polite" role="alert" style="position: initial !important;margin: 5px 5px 5px 5px;">
                                    <div class="toast toast-error" style="display: block;width: 100%;">
                                        <div class="toast-message">{{message.message}}</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <a class=" modal-action modal-close waves-effect waves-green btn-flat"><i class="large material-icons">swap_vert</i></a>
                </div>
            </div>
            <!-- Modal Structure end -->
            
            <!-- Place Search -->
            <div id="modal2" class="modal bottom-sheet">
                <div class="modal-content">
                    <div>
                        <input class="input" placeholder="Search Place" type="text" id="search_place" ng-keyup="searchPlace(text)" ng-model="text">
                    </div>
                    <div>
                        <ul class="collapsible popout" data-collapsible="accordion" watch>
                            <li ng-repeat="place in places">
                                <div class="collapsible-header"><img src={{place.icon}} style="height: 15px;margin: 0 auto;"></img>{{place.name}}</div>
                                <div class="collapsible-body"><p>{{place.formatted_address}}</p></div>
                            </li>
                        <ul>
                    </div>
                </div>
            </div>
            <!-- Place Search end -->


            <!-- Navvar -->
            <nav>
                <div class="nav-wrapper">
                    <a href="#" class="groupname brand-logo center"></a>
                    <ul id="nav-mobile" class="left hide-on-med-and-down">
                        <li><a href="sass.html">Sass</a></li>
                        <li><a href="badges.html">Components</a></li>
                        <li><a href="collapsible.html">JavaScript</a></li>
                    </ul>
                </div>
            </nav>
            <div class="mdl-tabs__panel is-active" id="candy_map_tab">
                <section class="container-full">
                    <div id="map"></div>
                </section>
            </div>
            <div class="fixed-action-btn horizontal click-to-toggle" style="bottom: 45px; right: 24px;">
                <a class="btn-floating btn-large red">
                    <i class="material-icons">menu</i>
                </a>
                <ul>
                    <li><a class="btn-floating red" id="currentposition"><i class="large material-icons">my_location</i></a></li>
                    <li><a class="btn-floating yellow darken-1" id="messagebutton"><i class="large material-icons">comment</i></a></li>
                    <li><a class="btn-floating green" id="addlocationbutton"><i class="large material-icons">location_on</i></a></li>
                    <li><a class="btn-floating blue" id="messagelist"><i class="large material-icons">view_headline</i></a></li>
                    <li><a class="btn-floating pink" id="serchplacebutton"><i class="large material-icons">search</i></a></li>
                </ul>
            </div>
        </div>
    </div>
    <!-- Google Maps -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCO32axBK69PMVZMgs_oPjKMltoQa4pKI4&libraries=places"></script>
    <script src="../public/js/globalvariable.js"></script>
    <script src="../public/js/controll.js"></script>
    <script src="../public/js/firebase.js"></script>
    <script src="../public/js/googlemap.js"></script>
    <script src="../public/js/screenevent.js"></script>
    <script src="../public/js/popup.js"></script>
    
    <script>$('.collapsible').collapsible();</script>

</body>
</html>